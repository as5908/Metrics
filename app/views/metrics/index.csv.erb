<% headers = ["Status Id", "Text", "Accounts used","Followers Count","Time Taken(in milliseconds)","Failures","Creation Time"] %>
<%= CSV.generate_line(headers, {:row_sep => nil}).html_safe %>
<% @metrics.each do |m| %>
<% row = [ m.Status_id,m.Text,m.Accounts_Used,m.Followers_Count,m.Time,m.Failures,m.created_at ] %>
<%= CSV.generate_line(row, {:row_sep => nil}).html_safe %>
<% end %>
